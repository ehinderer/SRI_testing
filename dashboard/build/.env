# API options
API_HOST='"localhost"'    # hostname or URL of the SRI Testing API. quotes necessary as the variable substitutes as a macro into the build. default: '"localhost"'
API_PORT=8080             # port of the SRI Testing API. see cluster configuration for values. default: 8080
isAxiosMock=false         # use mock data and calls in UI build. default: false

# FEATURE FLAGS
_FEATURE_RUN_TESTS=false  # manual run tests button in UI. default: false
